Zadatak 1. JS -Arrays and Conditions
-------------
function countPrimeNumbers(from, to) {
    var filterPrim =[];
  for (var i = from; i <= to; i++) {
      var t =isPrime(i);
    if (t==1) {
        filterPrim.push(i);
    }
  }
  function isPrime(a){
    var k;
    if (!(a%2)) {
        return 0;
    }  

    for (k=3;k*k<a;k+=2){
     if (a%k==0) return 0;
    }
    return 1;  
   }
  console.log(filterPrim.length);
}
